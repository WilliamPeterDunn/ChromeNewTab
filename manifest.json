
{
  "manifest_version": 2,
  "name": "Custom new tab page",
  "version": "1.1",
  "description": "A custom new tab page for Chrome browser with random quotes",
  "permissions": ["contextMenus"],
  
    "chrome_url_overrides": {
        "newtab": "homepage.html"
    },

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
